{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "foreground": "#cba6f7",
          "style": "plain",
          "properties": {
            "style": "full"
          },
          "template": "\u001b[1m{{ .Path }}\u001b[0m "
        },
        {
          "type": "git",
          "foreground": "#ef9f76",
          "style": "plain",
          "properties": {
            "display_status": true,
            "display_stash_count": false
          },
          "template": "\u001b[1m{{ .HEAD }}\u001b[0m "
        },
        {
          "type": "text",
          "foreground": "#89b4fa",
          "style": "plain",
          "template": "\u001b[1m\u276f\u001b[0m "
        }
      ]
    }
  ],
  "version": 3
}
